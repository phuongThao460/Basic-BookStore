@model IEnumerable<BookStore.Models.CHUDE>

<ul class="list-group">
    <li class="list-group-item active" style="text-align: center; font-size: 20px">
        CHU DE SACH
    </li>
    @foreach (var item in Model)
    {
        <li class="list-group-item" style="font-size: 18px"><a href="@Url.Action("SachTheoChuDe","BookStore", new { id = item.MaCD})">@item.TenChuDe</a></li>
    }
</ul>
